 (!isDeleting && updatedText === fullText) {\n      setIsDeleting(true);\n      setIndex(prevIndex => prevIndex - 1);\n      setDelta(period);\n    } else if (isDeleting && updatedText === '') {\n      setIsDeleting(false);\n      setLoopNum(loopNum + 1);\n      setIndex(1);\n      setDelta(500);\n    } else {\n      setIndex(prevIndex => prevIndex + 1);\n    }\n  };\n\n  return (\n    <section className=\"banner\" id=\"home\">\n      <Container>\n        <Row className=\"aligh-items-center\">\n          <Col xs={12} md={6} xl={7}>\n            <TrackVisibility>\n              {({ isVisible }) =>\n              <div className={isVisible ? \"animate__animated animate__fadeIn\" : \"\"}>\n                <span className=\"tagline\">Welcome to My Portfolio</span>\n                <h1>{`Hi! I'm Gaurav Kumar`} <span className=\"txt-rotate\" dataPeriod=\"1000\" data-rotate='[ \"React Native Developer\", \"MERN Stack Developer\", \"Salesforce Enthusiast\" ]'><span className=\"wrap\">{text}</span></span></h1>\n                <p>I’m a full-stack app developer passionate about building modern, scalable, and user-friendly applications. I specialize in React Native for cross-platform mobile apps, Express.js with MongoDB for robust backends, and I’m also exploring the power of Salesforce to enhance business workflows and CRM solutions.</p>\n                <button onClick={() => console.log('connect')}>Let’s Connect <ArrowRightCircle size={25} /></button>\n              </div>}\n            </TrackVisibility>\n          </Col>\n          <Col xs={12} md={6} xl={5}>\n            <TrackVisibility>\n              {({ isVisible }) =>\n                <div className={isVisible ? \"animate__animated animate__zoomIn\" : \"\"}>\n                  <img src={headerImg} alt=\"Header\" />\n                </div>}\n            </TrackVisibility>\n          </Col>\n        </Row>\n      </Container>\n    </section>\n  );\n};\n","/home/singsys/personal-portfolio/client/src/components/ProjectCa